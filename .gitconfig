[user]
	email = fog.code000@gmail.com
	name = enix403
[alias]
	br = branch
	st = status
	co = checkout
	cm = commit
	sw = switch
[pager]
	branch = false
[init]
	defaultBranch = master
[diff]
	tool = meld
[difftool]
    prompt = false
[difftool "meld"]
    cmd = meld "$LOCAL" "$REMOTE"
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential]
	helper = store
