export EDITOR=vim

# To Change cursor
#   echo -e "\e[6 q"
#
# Other options (replace the number after \e[):
#
# Ps = 0  -> blinking block.
# Ps = 1  -> blinking block (default).
# Ps = 2  -> steady block.
# Ps = 3  -> blinking underline.
# Ps = 4  -> steady underline.
# Ps = 5  -> blinking bar (xterm).
# Ps = 6  -> steady bar (xterm).


